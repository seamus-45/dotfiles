! для применения настроек в текущей сессии используйте `xrdb -load ~/.Xresources`

URxvt.geometry: 100x34
!URxvt.termName: rxvt
URxvt*loginShell: true

URxvt.perl-lib: /home/fedotov_sv/.config/urxvt/perl
URxvt.perl-ext-common: default,url-select
!URxvt.tabbed.tabbar-fg: 0
!URxvt.tabbed.tabbar-bg: 7
!URxvt.tabbed.tab-fg:    15
!URxvt.tabbed.tab-bg:    12
!URxvt.tabbed.title-fg:  0
!URxvt.tabbed.title-bg:  7
URxvt.keysym.M-u:perl:url-select:select_next
!URxvt.searchable-scrollback: M-space

URxvt.pointerBlank: true
URxvt.pointerBlankDelay: 3
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.visualBell: false
URxvt.urgentOnBell: True

URxvt.depth: 32
!URxvt.background: rgba:04f0/0fff/0fff/cccc
!URxvt.cursorColor: lightcyan3
!URxvt.underlineColor: white
!Rxvt.shading: 70
!Rxvt.tintColor: cyan
URxvt.inheritPixmap: false
URxvt.internalBorder: 3
URxvt.lineSpace: 0

! font
URxvt.allow_bold: true
URxvt.font: xft:Droid Sans Mono Slashed:pixelsize=15
URxvt.boldFont: xft:Droid Sans Mono Slashed:bold

! Fix font space
URxvt*letterSpace: -1

! scroll settings
URxvt.scrollBar: false
URxvt.scrollstyle: plain
URxvt.scrollBar_right: true
URxvt.scrollColor: #1A1A1A
URxvt.scrollTtyKeypress: false
URxvt.scrollTtyOutput: false
URxvt.jumpScroll: false
URxvt.scrollWithBuffer: false
URxvt.saveLines: 0

! color the man
URxvt.colorMode: on
URxvt.boldColors: on
URxvt.dynamicColors: on
URxvt.underLine: on
URxvt.colorBDMode: on
URxvt.colorBD:  green
URxvt.colorULMode: on
URxvt.colorUL: #6495ED
URxvt.underlineURLs: true
URxvt.urlLauncher: /usr/bin/chromium-browser

! colors
URxvt.intensityStyles: false

! base16 theme (https://github.com/chriskempson/base16-xresources/blob/master/base16-default.dark.xresources)
#define base00 #151515
#define base01 #202020
#define base02 #303030
#define base03 #505050
#define base04 #b0b0b0
#define base05 #d0d0d0
#define base06 #e0e0e0
#define base07 #f5f5f5
#define base08 #ac4142
#define base09 #d28445
#define base0A #f4bf75
#define base0B #90a959
#define base0C #75b5aa
#define base0D #6a9fb5
#define base0E #aa759f
#define base0F #8f5536

*.foreground:   base05
*.background:   base00
*.cursorColor:  base05

*.color0:       base00
*.color1:       base08
*.color2:       base0B
*.color3:       base0A
*.color4:       base0D
*.color5:       base0E
*.color6:       base0C
*.color7:       base05

*.color8:       base03
*.color9:       base08
*.color10:      base0B
*.color11:      base0A
*.color12:      base0D
*.color13:      base0E
*.color14:      base0C
*.color15:      base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*.color16:      base09
*.color17:      base0F
*.color18:      base01
*.color19:      base02
*.color20:      base04
*.color21:      base06


